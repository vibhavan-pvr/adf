					"Snowflake1_password": {
						"value": "[parameters('Snowflake1_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"Snowflake1_properties_typeProperties_database": {
						"value": "[parameters('Snowflake1_properties_typeProperties_database')]"
					},
					"Snowflake1_properties_typeProperties_role": {
						"value": "[parameters('Snowflake1_properties_typeProperties_role')]"
					},
					"Snowflake1_properties_typeProperties_host": {
						"value": "[parameters('Snowflake1_properties_typeProperties_host')]"
					},
					"sample_rest_service_properties_typeProperties_url": {
						"value": "[parameters('sample_rest_service_properties_typeProperties_url')]"
					},
					"staging_service_sasUri": {
						"value": "[parameters('staging_service_sasUri')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "pvrdf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"local_server_password": {
						"value": "[parameters('local_server_password')]"
					},
					"local_server_properties_typeProperties_server": {
						"value": "[parameters('local_server_properties_typeProperties_server')]"
					},
					"local_server_properties_typeProperties_database": {
						"value": "[parameters('local_server_properties_typeProperties_database')]"
					},
					"local_server_properties_typeProperties_userName": {
						"value": "[parameters('local_server_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'pvrdf_ArmTemplate_0')]"
			]
		}
	]
}